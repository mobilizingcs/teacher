<campaign>
	<surveys>
		<survey>
			<id>personalitycolor</id>
			<title>Personality Color</title>
			<description>Share the data from the results of your True Colors personality test plus other information about you.</description>
			<submitText>Thanks! Your response has been submitted.</submitText>
			<anytime>true</anytime>
			<contentList>

				<prompt>
					<id>birth_gender</id>
					<displayLabel>Birth Gender</displayLabel>
					<promptText>Is your biological gender male or female?</promptText>
					<promptType>single_choice</promptType>
					<skippable>false</skippable>
					<properties>
						<property>
							<key>0</key>
							<label>Female</label>
						</property>
						<property>
							<key>1</key>
							<label>Male</label>
						</property>
					</properties>
				</prompt>

				<prompt>
					<id>p_color</id>
					<displayLabel>Predominant Color</displayLabel>
					<promptText>Enter your predominant personality color.</promptText>
					<promptType>single_choice</promptType>
					<properties>
						<property>
							<key>0</key>
							<label>Gold</label>
						</property>
						<property>
							<key>1</key>
							<label>Green</label>
						</property>
						<property>
							<key>2</key>
							<label>Orange</label>
						</property>
						<property>
							<key>3</key>
							<label>Blue</label>
						</property>
					</properties>
					<skippable>false</skippable>
					<skipLabel>Skip</skipLabel>
				</prompt>

				<prompt>
					<id>s_color</id>
					<displayLabel>Secondary Color</displayLabel>
					<promptText>Enter your secondary personality color.</promptText>
					<promptType>single_choice</promptType>
					<properties>
						<property>
							<key>0</key>
							<label>Gold</label>
						</property>
						<property>
							<key>1</key>
							<label>Green</label>
						</property>
						<property>
							<key>2</key>
							<label>Orange</label>
						</property>
						<property>
							<key>3</key>
							<label>Blue</label>
						</property>
					</properties>
					<skippable>false</skippable>
					<skipLabel>Skip</skipLabel>
				</prompt>
				
				<prompt>
					<id>orange</id>
					<displayLabel>Total orange score</displayLabel>
					<promptText>Enter the total score for orange.</promptText>
					<promptType>number</promptType>
					<skippable>false</skippable>
					<properties>
						<property>
							<key>min</key>
							<label>5</label>
						</property>
						<property>
							<key>max</key>
							<label>20</label>
						</property>
					</properties>
				</prompt>
				<prompt>
					<id>green</id>
					<displayLabel>Total green score</displayLabel>
					<promptText>Enter the total score for green.</promptText>
					<promptType>number</promptType>
					<skippable>false</skippable>
					<properties>
						<property>
							<key>min</key>
							<label>5</label>
						</property>
						<property>
							<key>max</key>
							<label>20</label>
						</property>
					</properties>
				</prompt>
				<prompt>
					<id>gold</id>
					<displayLabel>Total gold score</displayLabel>
					<promptText>Enter the total score for gold.</promptText>
					<promptType>number</promptType>
					<skippable>false</skippable>
					<properties>
						<property>
							<key>min</key>
							<label>5</label>
						</property>
						<property>
							<key>max</key>
							<label>20</label>
						</property>
					</properties>
				</prompt>
				<prompt>
					<id>blue</id>
					<displayLabel>Total blue score</displayLabel>
					<promptText>Enter the total score for blue.</promptText>
					<promptType>number</promptType>
					<skippable>false</skippable>
					<properties>
						<property>
							<key>min</key>
							<label>5</label>
						</property>
						<property>
							<key>max</key>
							<label>20</label>
						</property>
					</properties>
				</prompt>

				<prompt>
					<id>zodiac</id>
					<displayLabel>Zodiac sign</displayLabel>
					<promptText>What is your zodiac sign?</promptText>
					<promptType>single_choice</promptType>
					<properties>
						<property>
							<key>0</key>
							<label>Aquarius</label>
						</property>
						<property>
							<key>1</key>
							<label>Pisces</label>
						</property>
						<property>
							<key>2</key>
							<label>Aries</label>
						</property>
						<property>
							<key>3</key>
							<label>Taurus</label>
						</property>	
						<property>	
							<key>4</key>
							<label>Gemini</label>
						</property>
						<property>
							<key>5</key>
							<label>Cancer</label>
						</property>
						<property>
							<key>6</key>
							<label>Leo</label>
						</property>
						<property>
							<key>7</key>
							<label>Virgo</label>
						</property>
						<property>
							<key>8</key>
							<label>Libra</label>
						</property>
						<property>
							<key>9</key>
							<label>Scorpio</label>
						</property>
						<property>
							<key>10</key>
							<label>Sagittarius</label>
						</property>
						<property>
							<key>11</key>
							<label>Capricorn</label>
						</property>
					</properties>
					<skippable>false</skippable>
					<skipLabel>Skip</skipLabel>
				</prompt>
				
				<prompt>
					<id>sport</id>
					<displayLabel>Sports activity</displayLabel>
					<promptText>Are you involved in sports?</promptText>
					<promptType>single_choice</promptType>
					<properties>
						<property>
							<key>0</key>
							<label>Yes</label>
						</property>
						<property>
							<key>1</key>
							<label>No</label>
						</property>
					</properties>
					<skippable>false</skippable>
					<skipLabel>Skip</skipLabel>
				</prompt>
			</contentList>
		</survey>
	</surveys>
</campaign>