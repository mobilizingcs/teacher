<campaign>
    <surveys>
        <survey>
            <id>free_time</id>
            <title>Free Time</title>
            <description>Free Time of High School Students</description>
            <submitText>Thank you for participating.</submitText>
            <anytime>true</anytime>
            <contentList>
                <prompt>
                    <promptType>text</promptType>
                    <id>what</id>
                    <displayLabel>what</displayLabel>
                    <promptText>What are you doing right now (or what did you just finish doing if you waited for the activity to end before entering data)?</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>1</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>999</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>single_choice</promptType>
                    <id>type</id>
                    <displayLabel>type</displayLabel>
                    <promptText>What sort of activity is this?</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>1</key>
                            <label>Sports</label>
                        </property>
                        <property>
                            <key>2</key>
                            <label>Hobby</label>
                        </property>
                        <property>
                            <key>3</key>
                            <label>Socializing</label>
                        </property>
                        <property>
                            <key>4</key>
                            <label>Entertainment (Games/ Music)</label>
                        </property>
                        <property>
                            <key>5</key>
                            <label>School (Class/ Homework)</label>
                        </property>
                        <property>
                            <key>6</key>
                            <label>Social Media</label>
                        </property>
                        <property>
                            <key>7</key>
                            <label>Working</label>
                        </property>
                        <property>
                            <key>8</key>
                            <label>Family</label>
                        </property>
                        <property>
                            <key>9</key>
                            <label>Daily Necessities (Sleep/ Eat/ Grooming/ Chores/ Errands)</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>text</promptType>
                    <id>why</id>
                    <displayLabel>why</displayLabel>
                    <promptText>Why did you choose to do this activity? </promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>1</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>999</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>single_choice</promptType>
                    <id>where</id>
                    <displayLabel>where</displayLabel>
                    <promptText>Where did you do this activity?</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>1</key>
                            <label>Home</label>
                        </property>
                        <property>
                            <key>2</key>
                            <label>School</label>
                        </property>
                        <property>
                            <key>3</key>
                            <label>Work</label>
                        </property>
                        <property>
                            <key>4</key>
                            <label>Restaurant</label>
                        </property>
                        <property>
                            <key>5</key>
                            <label>Friend/ Relative's House</label>
                        </property>
                        <property>
                            <key>6</key>
                            <label>Vehicle</label>
                        </property>
                        <property>
                            <key>7</key>
                            <label>Party/ Event</label>
                        </property>
                        <property>
                            <key>8</key>
                            <label>Park</label>
                        </property>
                        <property>
                            <key>9</key>
                            <label>Other</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>single_choice</promptType>
                    <id>time_day</id>
                    <displayLabel>time_day</displayLabel>
                    <promptText>At what time of day did you do this activity? </promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>1</key>
                            <label>Morning</label>
                        </property>
                        <property>
                            <key>2</key>
                            <label>Noon</label>
                        </property>
                        <property>
                            <key>3</key>
                            <label>Afternoon</label>
                        </property>
                        <property>
                            <key>4</key>
                            <label>Evening</label>
                        </property>
                        <property>
                            <key>5</key>
                            <label>Night</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>single_choice</promptType>
                    <id>who</id>
                    <displayLabel>who</displayLabel>
                    <promptText>Who are you doing this with?</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>1</key>
                            <label>Alone</label>
                        </property>
                        <property>
                            <key>2</key>
                            <label>Family</label>
                        </property>
                        <property>
                            <key>3</key>
                            <label>Friends</label>
                        </property>
                        <property>
                            <key>4</key>
                            <label>Classmates</label>
                        </property>
                        <property>
                            <key>5</key>
                            <label>Co-workers</label>
                        </property>
                        <property>
                            <key>6</key>
                            <label>Other</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>number_with</id>
                    <displayLabel>number_with</displayLabel>
                    <promptText>How many people did this activity with you? </promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>999</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>false</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>length</id>
                    <displayLabel>length</displayLabel>
                    <promptText>How many minutes did you spend doing this? (1 hour = 60 minutes)</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>1</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>999</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>false</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>number</promptType>
                    <id>money</id>
                    <displayLabel>money</displayLabel>
                    <promptText>How much money did you spend to do this activity? (If free, type 0)</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>min</key>
                            <label>0</label>
                        </property>
                        <property>
                            <key>max</key>
                            <label>999</label>
                        </property>
                        <property>
                            <key>wholeNumber</key>
                            <label>false</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>single_choice</promptType>
                    <id>mood</id>
                    <displayLabel>mood</displayLabel>
                    <promptText>What is your mood right now?</promptText>
                    <skippable>false</skippable>
                    <properties>
                        <property>
                            <key>1</key>
                            <label>Excited</label>
                        </property>
                        <property>
                            <key>2</key>
                            <label>Happy</label>
                        </property>
                        <property>
                            <key>3</key>
                            <label>Calm</label>
                        </property>
                        <property>
                            <key>4</key>
                            <label>Bored</label>
                        </property>
                        <property>
                            <key>5</key>
                            <label>Sad</label>
                        </property>
                        <property>
                            <key>6</key>
                            <label>Stressed</label>
                        </property>
                        <property>
                            <key>7</key>
                            <label>Angry</label>
                        </property>
                    </properties>
                </prompt>
                <prompt>
                    <promptType>photo</promptType>
                    <id>photo</id>
                    <displayLabel>photo</displayLabel>
                    <promptText>Take a photo of what youâ€™re doing. (skippable)</promptText>
                    <skippable>true</skippable>
                    <skipLabel>Skip</skipLabel>
                    <properties>
                        <property>
                            <key>maxDimension</key>
                            <label>800</label>
                        </property>
                    </properties>
                </prompt>
            </contentList>
        </survey>
    </surveys>
</campaign>